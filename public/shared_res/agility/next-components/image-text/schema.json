  "type": "object",
  "title": "Image Text",
  "properties": {
    "copywriting": {
      "type": "object",
      "title": "Copy writing",
      "properties": {
        "position": {
          "type": "string",
          "title": "Alignment",
          "options": {
            "enum_titles": [
              "Left",
              "Right"
            ]
          },
          "enum": [
            "left",
            "right"
          ],
          "default": "left"
        },
        "hide_title": {
          "type": "string",
          "title": "Hide title?",
          "options": {
            "enum_titles": [
              "Yes",
              "No"
            ]
          },
          "enum": [
            "true",
            "false"
          ],
          "default": false
        },
        "heading": {
          "type": "string",
          "title": "Title",
          "default": "Title very very big how it is"
        },
        "subtitle": {
          "type": "string",
          "title": "Subtitle",
          "default": "Nice, the subtitle works great here too. Yay!"
        },
        "text": {
          "type": "string",
          "format": "html",
          "options": {
            "wysiwyg": false
          },
          "title": "Text",
          "default": "<p>We make uncompromising choices, one after another, to bring you exceptional coffee.</p><p>After all, we are the choices we make arenâ€™t we? We make uncompromising choices, one after another, to bring you exceptional coffee.</p><ul><li>List item 1</li><li>List item 2</li><li>List item 3</li><li>List item 4</li><li>List item 5</li></ul><blockquote>Give quality and flavor with free delivery</blockquote>"
        },
        "visual": {
          "type": "object",
          "title": "Main image",
          "description": "https://wiki.nespresso.com/x/-mR1CQ",
          "option": {
            "collapsed": true
          },
          "required": [
            "retina",
            "desktop",
            "mobile",
            "alt_text"
          ],
          "properties": {
            "alt_text": {
              "type": "string",
              "title": "Image set alternative text",
              "default": "Visual alternative text",
              "description": "Fill this field with a meaningful image description"
            },
            "mobile": {
              "type": "string",
              "title": "URL Image Set Mobile",
              "description": "URL to image 343 x 258 pixels (or 343 x 429 pixels if video)",
              "default": "https://www.nespresso.com/shared_res/agility/next-components/assets/image_text/visual_S.jpg"
            },
            "desktop": {
              "type": "string",
              "title": "URL Image Set Desktop",
              "description": "URL to image 680 x 509 pixels (or 680 x 383 pixels if video)",
              "default": "https://www.nespresso.com/shared_res/agility/next-components/assets/image_text/visual_L.jpg"
            },
            "retina": {
              "type": "string",
              "title": "URL Image Set Retina",
              "description": "URL to image 1360 x 1018 pixels (or 1360 x 766 pixels if video)",
              "default": "https://www.nespresso.com/shared_res/agility/next-components/assets/image_text/visual_XL.jpg"
            }
          },
          "default": "https://www.nespresso.com/shared_res/agility/next-components/assets/image_text/visual.jpg"
        },
        "visual_secondary": {
          "type": "object",
          "title": "Background decorative image",
          "description": "https://wiki.nespresso.com/x/-mR1CQ",
          "option": {
            "collapsed": true
          },
          "required": [
            "retina",
            "desktop",
            "mobile",
            "alt_text"
          ],
          "properties": {
            "alt_text": {
              "type": "string",
              "title": "Image set alternative text",
              "default": "Visual alternative text",
              "description": "Fill this field with a meaningful image description"
            },
            "mobile": {
              "type": "string",
              "title": "URL Image Set Mobile",
              "description": "URL to image 295 x 392 pixels (width x height)",
              "default": "https://www.nespresso.com/shared_res/agility/next-components/assets/image_text/visual_secondary_S.jpg"
            },
            "desktop": {
              "type": "string",
              "title": "URL Image Set Desktop",
              "description": "URL to image 424 x 424 pixels (width x height)",
              "default": "https://www.nespresso.com/shared_res/agility/next-components/assets/image_text/visual_secondary_L.jpg"
            },
            "retina": {
              "type": "string",
              "title": "URL Image Set Retina",
              "description": "URL to image 848 x 848 pixels (width x height)",
              "default": "https://www.nespresso.com/shared_res/agility/next-components/assets/image_text/visual_XL.jpg"
            }
